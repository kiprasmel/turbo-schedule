{
	"name": "@turbo-schedule/server",
	"version": "2.33.0",
	"description": "",
	"main": "dist/src/server.js",
	"typings": "dist/src/index.d.ts",
	"author": "Kipras Melnikovas <dev@kipras.org> (https://kipras.org)",
	"license": "",
	"private": true,
	"scripts": {
		"start": "yarn dev",
		"test": "yarn test:api --forceExit",
		"build": "     yarn build:ts && yarn build:content      && yarn build:docs && yarn after-build",
		"build:prod": "cross-env NODE_ENV=production yarn build:ts && yarn build:content:prod && yarn build:docs:prod && yarn after-build",
		"serve": "cross-env NODE_ENV=production node --max-http-header-size 64000 dist/script/start-server.js",
		"-----": "-----",
		"watch:ts": "tsc -b -w",
		"watch:server": "ts-node-dev ./src/script/start-server.ts",
		"dev": "concurrently --raw --kill-others --names \"0: tsc,1: server\" \"yarn watch:ts\" \"yarn watch:server\"",
		"test:api": "cross-env NODE_ENV=test PORT=5069  jest --config ./src/test/jest.config.js",
		"build:ts": "          yarn tsc -b -v                                                                   ",
		"build:content": "     yarn ts-node-dev ./src/script/scrape-content.ts   ",
		"build:content:prod": "cross-env NODE_ENV=production     node        ./dist/script/scrape-content.js          # depends on `build:ts`",
		"build:docs": "        yarn ts-node-dev      ./src/script/generate-openapi-docs.ts                          ",
		"build:docs:prod": "cross-env NODE_ENV=production        node        ./dist/script/generate-openapi-docs.js   # depends on `build:ts`",
		"after-build": "node ./dist/script/build-info.js store"
	},
	"dependencies": {
		"@turbo-schedule/common": "^2.33.0",
		"@turbo-schedule/database": "^2.33.0",
		"@turbo-schedule/scraper": "^2.33.0",
		"@types/cors": "2.8.6",
		"@types/cron": "1.7.1",
		"@types/debug": "4.1.5",
		"@types/express": "4.17.2",
		"@types/fs-extra": "8.0.0",
		"@types/helmet": "0.0.45",
		"@types/json-server": "0.14.2",
		"@types/morgan": "1.7.37",
		"@types/node": "12.12.7",
		"@types/node-fetch": "2.5.3",
		"cors": "2.8.5",
		"cron": "1.7.2",
		"cross-env": "6.0.3",
		"debug": "4.1.1",
		"express": "4.17.1",
		"express-oas-generator": "1.0.23",
		"fs-extra": "8.1.0",
		"helmet": "3.20.1",
		"json-server": "0.16.1",
		"morgan": "1.9.1",
		"node-fetch": "2.6.0",
		"openapi-types": "1.3.5",
		"redoc": "2.0.0-rc.18",
		"rotating-file-stream": "1.4.6"
	},
	"devDependencies": {
		"@types/jest": "24.0.23",
		"@types/supertest": "2.0.8",
		"concurrently": "5.0.0",
		"cross-env": "6.0.3",
		"jest": "24.9.0",
		"supertest": "4.0.2",
		"ts-jest": "24.1.0",
		"ts-node-dev": "1.0.0-pre.44",
		"typescript": "4.3.5"
	},
	"resolutions": {
		"express-oas-generator/swagger-ui-express": "4.1.2"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/kiprasmel/turbo-schedule.git",
		"directory": "server/"
	}
}
